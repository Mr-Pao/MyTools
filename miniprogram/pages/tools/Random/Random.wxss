.container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100vh;
  background: var(--background-color-black);
}

.wrapper {
  width: 100%;
  height: 50%;
  /* border-radius: 5px; */
  margin-top:-110%;
}

.text {
  flex: 0 0 100%;
  font-size: 15rem;
  font-weight: 900;
  color: #00000000;
  text-align: center;
  font-family: 'Lato', sans-serif;
  position: absolute;
  left: 50%;
  top: 30%;
  transform: translate(-50%, -50%);
  border-bottom: 1px solid #d4d7ff;
  /* border-top: 1px solid #d4d7ff; */
  background: url("data:image/gif;base64,");
  background-clip: text;
  -webkit-background-clip: text;
}

.text:after {
  content: attr(data-text);
  -webkit-text-stroke: 1.5px #d4d7ff;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -49%);
  background: url("data:image/gif;base64,");
  background-clip: text;
  -webkit-background-clip: text;
  background-size: 43%;
}

.form {
  display: flex;
  flex-direction: row;
  align-items: center; /* 垂直居中 */
  justify-content: space-between;
  margin-top: 20%;
  width: 50%;
  height: 100rpx;
}
.input-wrapper {
  text-align: center;
  font-size: 50rpx;
  color: var(--text-color2);
}

.btn {
  position: absolute;
  font-size: 18px;
  color: #fff;
  background-color: var(--theme-color);
  border: none;
  border-radius: 20px;
  padding: 10px;
  left: 30%;
  bottom: 15%;
}

.btn:active {
  background-color: #ccc;
}

.refresh{
  position: fixed;
	bottom: 14.5%;
	right: 80rpx;
  width: 80rpx;
}
.rotating-icon {
  animation: rotation 0.5s linear infinite;
}
@keyframes rotation {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}